# Example environment variables for Rabit HQ
# Copy to .env and fill values

DATABASE_URL=postgresql://user:password@localhost:5432/rabit_hq?schema=public
NEXTAUTH_SECRET=change_this_to_a_long_random_value
NEXTAUTH_URL=http://localhost:3000
FEATURE_ENABLE_AI=true
OPENAI_API_KEY=               # optional for AI insights (OpenAI)
DEEPSEEK_API_KEY=             # optional for AI insights (DeepSeek)
DEEPSEEK_MODEL=deepseek-chat  # optional override
DEEPSEEK_API_URL=https://api.deepseek.com
FEATURE_ENABLE_INVESTOR=true
