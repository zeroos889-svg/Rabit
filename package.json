{
  "name": "rabit",
  "private": true,
  "version": "0.1.0",
  "description": "Rabit HR platform frontend and supporting tooling",
  "type": "module",
  "scripts": {
    "dev": "vite --config client/vite.config.ts",
    "dev:server": "tsx watch server/_core/index.ts",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:server\" --names \"client,server\" --prefix-colors \"cyan,magenta\"",
    "build": "vite build --config client/vite.config.ts",
    "build:server": "tsc -p server/tsconfig.json",
    "preview": "vite preview --config client/vite.config.ts",
    "test": "vitest --config client/vite.config.ts",
    "test:ui": "vitest --ui --config client/vite.config.ts",
    "lint": "tsc -p client/tsconfig.json --noEmit",
    "lint:check": "eslint . --ext .ts,.tsx --max-warnings 0",
    "build:all": "npm run build && npm run build:server",
    "db:push": "drizzle-kit push:mysql",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate:mysql"
  },
